{
   "version": 2,
   "builds": [
      {
         "src": "/BACK/index.js",
         "use": "@vercel/node"
      },
      {
         "src": "/FRONT/package.json",
         "use": "@vercel/static-build",
         "config": {
            "distDir": "dist"
         }
      }
   ],
   "rewrites": [{ "source": "/FRONT/(.*)", "destination": "/FRONT/" }],
   "routes": [
      {
         "src": "/BACK/(.*)",
         "dest": "/BACK/"
      }
   ]
}
